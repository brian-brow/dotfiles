{
    "Created": "Phunt_Vieg_",
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "white":    "#CDD6F4",
        "black":    "#181825",
        "blue":     "#89B4FA",
        "lavender": "#B4BEFE",
        "cyan":     "#D3F2EF",
        "green":    "#A6E3A1",
        "purple":   "#231B30",
        "red":      "#F38BA8",
        "pink":     "#F5C2E7",
        "yellow":   "#F9E2AF",
        "orange":   "#FAB387"
    },
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "p:white",
                    "style": "plain",
                    "template": "\u256d\u2500",
                    "type": "text"
                },
                {
                    "background": "p:black",
                    "foreground": "p:cyan",
                    "leading_diamond": "\ue0b6",
                    "style": "diamond",
                    "template": " {{ .UserName }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "session"
                },
                {
                    "background": "p:black",
                    "foreground": "p:cyan",
                    "properties": {
                        "style": "full"
                    },
                    "style": "diamond",
                    "template": " \uf114  {{ .Path }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "path"
                },
                {
                    "background": "p:cyan",
                    "foreground": "p:black",
                    "properties": {
                        "branch_icon": "",
                        "fetch_stash_count": true,
                        "fetch_status": false,
                        "fetch_upstream_icon": true
                    },
                    "style": "diamond",
                    "template": " \ue725({{ .UpstreamIcon }} {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
                    "trailing_diamond": "\ue0b0",
                    "type": "git"
                },
                {
                    "background": "p:black",
                    "foreground": "p:cyan",
                    "properties": {
                        "time_format": "15:04"
                    },
                    "style": "diamond",
                    "template": "{{ if ne (.Segments.Contains \"Git\") true }} {{ .CurrentDate | date .Format }} {{ end }}",
                    "trailing_diamond": "\ue0b0",
                    "type": "time"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "p:cyan",
                    "style": "plain",
                    "template": "\u2570\u2500\u25B6</>",
                    "type": "text"
                }
            ]
        }
    ],
    "transient_prompt": {
        "template": "<p:red>\uf489 </><p:blue>\uf0da</><p:pink>\uf105</> "
    },
    "final_space": true,
    "version": 7
}
